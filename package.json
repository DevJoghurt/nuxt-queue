{
  "name": "nvent-monorepo",
  "version": "0.5.0",
  "description": "Event-driven workflows for Nuxt - Monorepo",
  "repository": "DevJoghurt/nvent",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm prepare && pnpm --filter \"./packages/**\" prepack",
    "build:stub": "pnpm prepare",
    "prepare": "pnpm --filter \"./packages/**\" prepack --stub && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "test": "vitest run",
    "test:unit": "vitest test/unit",
    "test:e2e": "vitest test/e2e",
    "test:coverage": "vitest --coverage",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs,.vue .",
    "cleanup": "rimraf dist*/ **/node_modules .nuxt* playground/.data packages/**/dist",
    "bumpp": "bumpp --recursive"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.11.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "4.2.1",
    "@nuxt/test-utils": "^3.20.1",
    "@types/node": "^24.10.1",
    "bumpp": "10.3.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.11",
    "nuxi": "^3.30.0",
    "nuxt-mcp": "0.2.6",
    "rimraf": "^6.1.2",
    "typescript": "latest",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.5"
  },
  "packageManager": "pnpm@10.20.0"
}
