{
  "name": "nuxt-queue",
  "version": "0.0.7",
  "description": "Nuxt queue service based on Bullmq",
  "repository": "DevJoghurt/nuxt-fhir",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn dev:prepare && nuxt-module-build build",
    "prepack": "yarn build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "cleanup": "rimraf 'playground/node_modules' 'node_modules'",
    "redis": "./.local/redis.bat",
    "bumpp": "bumpp"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "rollup": "^4.20.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-circular-dependencies": "^1.1.2",
    "@rollup/plugin-alias": "^5.1.0",
    "unimport": "^3.10.0",
    "magicast": "^0.3.4",
    "defu": "^6.1.4",
    "bullmq": "^5.12.1",
    "pm2": "^5.4.2",
    "zod": "^3.23.8",
    "@nuxt/ui": "^2.18.4",
    "@iconify-json/heroicons": "1.1.24",
    "@vercel/nft": "^0.27.2",
    "semver": "^7.6.2",
    "mlly": "^1.7.1",
    "pkg-types": "^1.1.3",
    "globby": "14.0.1",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.5.0",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "@types/node": "^22.1.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.8.0",
    "nuxt": "^3.12.4",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29",
    "bumpp": "9.4.2"
  }
}
