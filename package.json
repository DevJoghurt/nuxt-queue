{
  "name": "nuxt-queue",
  "version": "0.1.8",
  "description": "Nuxt queue service based on Bullmq",
  "repository": "DevJoghurt/nuxt-queue",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn dev:prepare && nuxt-module-build build",
    "prepack": "yarn build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "cleanup": "rimraf 'playground/node_modules' 'node_modules'",
    "bumpp": "bumpp"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "nuxt": "^3.15.4",
    "nuxi": "^3.22.2",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/ui": "3.0.0-alpha.13",
    "rollup": "^4.34.8",
    "rollup-plugin-esbuild": "^6.2.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-circular-dependencies": "^1.1.2",
    "@rollup/plugin-alias": "^5.1.1",
    "json-editor-vue": "^0.17.3",
    "unimport": "^3.14.5",
    "magicast": "^0.3.5",
    "defu": "^6.1.4",
    "bullmq": "^5.41.5",
    "zod": "^3.24.2",
    "@vercel/nft": "^0.29.2",
    "semver": "^7.7.1",
    "mlly": "^1.7.4",
    "pkg-types": "^1.3.1",
    "globby": "14.1.0",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "1.2.2",
    "@nuxt/devtools": "^2.1.0",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "3.15.4",
    "@nuxt/test-utils": "^3.17.0",
    "@types/node": "^22.13.5",
    "changelogen": "^0.5.7",
    "eslint": "^9.21.0",
    "typescript": "latest",
    "vitest": "^3.0.6",
    "vue-tsc": "^2.2.4",
    "bumpp": "10.0.3"
  }
}
