{
  "name": "nuxt-queue",
  "version": "0.1.10",
  "description": "Nuxt queue service based on Bullmq",
  "repository": "DevJoghurt/nuxt-queue",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn dev:prepare && nuxt-module-build build",
    "prepack": "yarn build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "cleanup": "rimraf 'playground/node_modules' 'node_modules'",
    "bumpp": "bumpp"
  },
  "resolutions": {
    "nuxt": "^4.0.3",
    "@nuxt/ui": "3.3.0"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.3",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.4",
    "bullmq": "^5.62.0",
    "defu": "^6.1.4",
    "globby": "15.0.0",
    "ioredis": "^5.8.2",
    "jiti": "^2.6.1",
    "json-editor-vue": "^0.18.1",
    "mlly": "^1.8.0",
    "node-ts": "8.0.8",
    "nuxt": "4.1.3",
    "pathe": "^2.0.3",
    "perfect-debounce": "^2.0.0",
    "pkg-types": "^2.3.0",
    "semver": "^7.7.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@iconify-json/devicon": "^1.2.45",
    "@iconify-json/heroicons": "1.2.3",
    "@iconify-json/lucide": "^1.2.71",
    "@nuxt/devtools": "^3.0.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "4.1.3",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxt/ui": "4.1.0",
    "@types/node": "^24.9.1",
    "bumpp": "10.3.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "nuxi": "^3.29.3",
    "nuxt-mcp": "0.2.5",
    "typescript": "latest",
    "vitest": "^4.0.4",
    "vue-tsc": "^3.1.2"
  },
  "packageManager": "yarn@4.10.3"
}
